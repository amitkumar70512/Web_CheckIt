<main id="main">
    <!-- ======= breadcrumbs Section c======= -->
    
      <section class="breadcrumbs">
          <div class="container">
              <div class="d-flex justify-content-between align-items-center">
                <h2></h2>
                <ol>
                  <li>
                  <button type="button" class="btn btn-primary" data-toggle="collapse" data-target="#" style="margin:3px;">Check</button>
                </li>
                <li>
                  <button type="button" class="btn btn-primary" data-toggle="collapse" data-target="#" style="margin:3px;">Absent</button>
                </li>
                <li>
                  <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#presentModal" style="margin:3px;">Present</button>
                </li>
                <li>
                  <button type="button" class="btn btn-primary" data-toggle="collapse" data-target="#allstudents"style="margin:3px;">Students</button>
                </li>
                <li>
                  <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#myModal"style="margin:3px;">Find</button>
                </li>           
                </ol>
              </div>
          </div>
      </section>
    
      
        <div class="container">
          <% if(typeof alert != 'undefined') { %>
            <% alert.forEach(function(error) { %>
                <div class="alert alert-warning alert-dismissible fade show" role="alert">
                    <%= error.msg %>
                    <button  type="submit" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                    
                    </button>
                  </div>
            <% }) %>
        <% } %>
        </div>
    
    
     <div id="allstudents" class="collapse">
           
      <section class="features" data-aos="fade-up" date-aos-delay="300">
        <div class="container">
          <div class="section-title">
            <h2><i class="far fa-user-circle fa-lg"></i></h2>
            </div>
          <div class="btn-group-horizontal" >
            <button id="amit" type="button" class="btn btn-primary" data-toggle="collapse" data-target="#5A">5A</button>
            <button type="button" class="btn btn-primary" data-toggle="collapse" data-target="#5B">5B</button>
            <button type="button" class="btn btn-primary" data-toggle="collapse" data-target="#5C">5C</button>
          </div>
        </div>
      </section>
    </div>
    
      <div class="container">
       
          <div id="5A" class="collapse">
            
            <table class="table "data-aos="fade-up" date-aos-delay="300">
              <thead>
                <tr>
                  <th scope="col">'5A'</th>
                  <th scope="col">Name</th>
                  <th scope="col">USN</th>
                  <th scope="col">Email</th>
                </tr>
              </thead>
              
            <% for (var i =0; i <=countA;  i++ ) { %>
    
              <% if(typeof StudentsA[i] != 'undefined') { %>
                  
                  <tbody>
                      <tr>
                        <th scope="row">
                          <%=i+1%>
                        </th>
                    <td>
                    <%=  StudentsA[i].name%>
                    </td>
                    <td>
                    <%=  StudentsA[i].usn%>
                    </td>
                    <td>
                    <%=  StudentsA[i].email%>
                    </td>
                    </tr>
                    
                  </tbody>
                   
              <% } %>
             
            <%  }%>
        

            </table>
      
          </div> 
    </div>
    
     <!-- for 5B  -->
