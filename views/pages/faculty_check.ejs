            <tr>
              <th scope="col">'5B'</th>
              <th scope="col">Name</th>
              <th scope="col">USN</th>
              <th scope="col">Email</th>
            </tr>
          </thead>
         
        <% for (var i =0; i <=countB;  i++ ) { %>

          <% if(typeof StudentsB[i] != 'undefined') { %>
              
              <tbody>
                  <tr>
                    <th scope="row">
                      <%=i+1%>
                    </th>
                <td>
                <%=  StudentsB[i].name%>
                </td>
                <td>
                <%=  StudentsB[i].usn%>
                </td>
                <td>
                <%=  StudentsB[i].email%>
                </td>
                </tr>
                
              </tbody>
               
          <% } %>
         
        <%  }%>
    

        </table>
  
      </div> 
